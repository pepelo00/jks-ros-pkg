<package>
  <description>
    A ros node to detect the buttons on an elevator call panel.
  </description>
  <author>Blake Carpenter</author>
  <license>BSD</license>
  <depend package="roscpp"/>
  <depend package="std_msgs"/>
  <depend package="stair_msgs"/>
  <rosdep name="opencv2" />


  <platform os="ubuntu" version="9.04"/>
  <platform os="ubuntu" version="9.10"/>
  <platform os="ubuntu" version="10.04"/>
  <export>
    <cpp lflags="-L${prefix}/lib_find_buttons -l_find_buttons"/>
  </export>
  <url></url>
</package>
